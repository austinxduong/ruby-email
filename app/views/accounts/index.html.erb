<div class="title-container">
<nav class="navbar ">
    <h1>All accounts ^*</h1>
    <%= link_to "Home Page", accounts_path, class: "btn btn-default" %> 
    <%= link_to "Create a New Account ✨", new_account_path, class: "btn btn-default" %> 
</nav>
</div>




<div class="account-container">
<ul>
    <% @accounts.each do |account| %>
        <li>

            <a href="<%= account_path(account) %>">
                <p>View Account 📖: 🧚 username = '<%= account.username%>' [📧 email= <%=account.email %>] - [🌱 created at: <%= account.created_at %>] - [🪴💧☀️ last updated: <%= account.updated_at %>]</p>
            </a>
        </li>
    <% end %>
<ul>
</div>
</div>
