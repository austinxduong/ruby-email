<h1>All accounts</h1>
<p>home page</p>


<%= link_to "Create a New Account ✨", new_account_path %> 

<ul>
    <% @accounts.each do |account| %>
        <li>

            <a href="<%= account_path(account) %>">
                <p>View Account 📖: 🧚 username = '<%= account.username%>' [📧 email= <%=account.email %>] - [🌱 created at: <%= account.created_at %>] - [🪴💧☀️ last updated: <%= account.updated_at %>]</p>
            </a>
        </li>
    <% end %>
